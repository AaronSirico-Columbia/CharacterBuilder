<Window x:Class="CharacterBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CharacterBuilder"
        xmlns:views="clr-namespace:CharacterBuilder.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <views:UserControlCharacter x:Name="ucCharacter"/>
        </StackPanel>
        <StackPanel Margin="410,-3,-410,3">
            <views:UserControlCharacter x:Name="ucCharacter2"/>
        </StackPanel>
    </Grid>
</Window>
